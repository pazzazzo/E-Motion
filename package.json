{
  "name": "e-motion_dashboard",
  "version": "0.0.1",
  "description": "Dashboard of Hydix E-Motion",
  "main": "main.js",
  "homepage": "https://pazzazzo.github.io/e-motion",
  "scripts": {
    "test": "UV_THREADPOOL_SIZE=$(nproc) electron . -dev --no-zygote --no-sandbox",
    "start": "UV_THREADPOOL_SIZE=$(nproc) electron . --no-zygote --no-sandbox",
    "perf": "chrt -r 99 npm test",
    "version": "electron -v",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": {
    "name": "pazzazzo",
    "email": "azzo.lorenzo@gmail.com"
  },
  "license": "CC-BY-NC-ND-4.0",
  "dependencies": {
    "@google-cloud/speech": "^6.7.1",
    "@tensorflow/tfjs-node": "^4.22.0",
    "chart.js": "^4.4.1",
    "deepspeech": "^0.9.3",
    "ffi-napi": "^2.5.0",
    "google-tts-api": "^0.0.6",
    "gpt4js": "^1.7.8",
    "jsqr": "^1.4.0",
    "mapbox": "^1.0.0-beta10",
    "mapbox-gl": "^3.11.0",
    "mfcc": "^0.0.3",
    "node-record-lpcm16": "^1.0.1",
    "node-wifi": "^2.0.16",
    "path": "^0.12.7",
    "qrcode-reader": "^1.0.4",
    "qrcode-terminal": "^0.12.0",
    "request": "^2.88.2",
    "signal-windows": "^0.0.1",
    "simple-keyboard": "^3.8.38",
    "spotify-web-api-node": "^5.0.2",
    "usb": "^2.13.0",
    "vosk-browser": "^0.0.8",
    "wav-decoder": "^1.3.0"
  },
  "build": {
    "productName": "HydixEMotion",
    "appId": "fr.hydix.e-motion",
    "directories": {
      "output": "dist"
    },
    "linux": {
      "target": [
        "deb"
      ],
      "category": "System"
    }
  },
  "devDependencies": {
    "electron": "github:castlabs/electron-releases#v33.4.3+wvcus",
    "electron-builder": "^24.13.3",
    "express": "^4.21.2"
  }
}
